	"item": [
		{
			"name": "Add Book Ankush",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const company=pm.globals.get(\"companyCode\");\r",
							" const ran=pm.variables.replaceIn('{{$randomInt}}');\r",
							" console.info(ran)\r",
							"pm.collectionVariables.set(\"ISBN\",company+ran);\r",
							"pm.iterationData.set(\"ISBN\",company+ran);\r",
							"const book=pm.iterationData.get(\"BOOKNAME\");\r",
							"pm.collectionVariables.set(\"Book_Name\",book);\r",
							"\r",
							"const author=pm.iterationData.get(\"AuthorName\");\r",
							"pm.collectionVariables.set(\"Author_name\",author);\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"\r",
							"const authorname=pm.collectionVariables.get(\"Author_name\");\r",
							"console.log(authorname)\r",
							"const JSONDATA=pm.response.json();\r",
							"console.log(JSONDATA);\r",
							"const bookid=JSONDATA.ID;\r",
							"pm.collectionVariables.set(\"BOOK_ID\",bookid);"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n\"name\":\"{{Book_Name}}\",\r\n\"isbn\":\"{{ISBN}}\",\r\n\"aisle\":\"111\",\r\n\"author\":\"{{Author_name}}\"\r\n}\r\n",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{base_url}}/Library/Addbook.php",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"Library",
						"Addbook.php"
					]
				}
			},
			"response": []
		},
